@import './Theme.pcss';

#main-header {
  font-family: helvetica, arial, sans-serif;
  font-size: 22px;
  color: $company-color;
  background-color: #222;
  padding: 5px;

  img {
    width: 40px;
    height: 40px;
    vertical-align: middle;
